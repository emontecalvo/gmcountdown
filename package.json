{
  "name": "GMCountdown",
  "version": "0.0.0",
  "description": "Going Merry Countdown",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "build": "mkdir -p build && cp client/*.css client/*.html client/*.ico build/ && webpack",
    "watch": "npm run build && run-p watch:*",
    "watch:copy": "chokidar \"client/*.css\" \"client/*.html\" \"client/*.ico\" -c \"cp {path} build/\"",
    "watch:js": "webpack --watch",
    "watch:server": "nodemon -w server.js server.js -x \"npm run\"",
    "server.js": "npm start",
    "test": "mocha --compilers js:babel-register --require babel-polyfill",
    "deploy": "npm run build&&gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emontecalvo/gmcountdown.git"
  },
  "author": "Emily Monte Calvo",
  "license": "MIT",
  "homepage": "https://emontecalvo.github.io/gmcountdown",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "mongo": "^0.1.0",
    "mongodb": "^2.2.15",
    "mongoose": "^4.7.2",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-image-slider": "^0.1.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^6.0.3",
    "webpack": "^2.0.0",
    "xml2js": "^0.4.17",
    "xml2json": "^0.11.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "enzyme": "^2.8.0",
    "gh-pages": "^0.12.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^1.13.3"
  }
}
